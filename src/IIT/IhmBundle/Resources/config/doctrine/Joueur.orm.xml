<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IIT\IhmBundle\Entity\Joueur" table="joueur">
    <indexes>
      <index name="idEquipe" columns="idEquipe"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="20" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="20" nullable="false"/>
    <field name="nationalite" type="string" column="nationalite" length="20" nullable="false"/>
    <field name="datenaiss" type="date" column="dateNaiss" nullable="false"/>
    <field name="taille" type="integer" column="taille" nullable="false"/>
    <field name="poids" type="integer" column="poids" nullable="false"/>
    <field name="image" type="string" column="image" length="150" nullable="false"/>
    <many-to-one field="idequipe" target-entity="Equipe">
      <join-columns>
        <join-column name="idEquipe" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
