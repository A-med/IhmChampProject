{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Resultat list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Buteq1</th>
                <th>Buteq2</th>
                <th>Pointeq1</th>
                <th>Pointeq2</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('resultat_show', { 'id': entity.id }) }}">{{ entity.buteq1 }}</a></td>
                <td>{{ entity.buteq2 }}</td>
                <td>{{ entity.pointeq1 }}</td>
                <td>{{ entity.pointeq2 }}</td>
                <td>{{ entity.id }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('resultat_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('resultat_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('resultat_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
